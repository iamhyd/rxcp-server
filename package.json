{
  "name": "rxcp-server",
  "private": true,
  "version": "1.0.0",
  "description": "API for rAthena servers",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config ./src/config/ormconfig.js",
    "migration:create": "npm run typeorm migration:create -- -n",
    "migration:generate": "npm run typeorm migration:generate -- -n",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "schema:sync": "npm run typeorm schema:sync",
    "schema:log": "npm run typeorm schema:log",
    "schema:drop": "npm run typeorm schema:drop",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config -n ./src/config/ormconfig.js",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -n ./src/config/ormconfig.js"
  },
  "dependencies": {
    "@nestjs/common": "7.0.8",
    "@nestjs/config": "0.4.0",
    "@nestjs/core": "7.0.8",
    "@nestjs/jwt": "7.0.0",
    "@nestjs/passport": "7.0.0",
    "@nestjs/platform-fastify": "7.0.8",
    "@nestjs/swagger": "4.5.1",
    "@nestjs/typeorm": "7.0.0",
    "@types/bcryptjs": "2.4.2",
    "bcryptjs": "2.4.3",
    "class-transformer": "0.2.3",
    "class-validator": "0.11.1",
    "fastify": "2.13.1",
    "fastify-swagger": "2.5.1",
    "mysql": "2.18.1",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.5.5",
    "typeorm": "0.2.24",
    "typeorm-seeding": "1.5.0"
  },
  "devDependencies": {
    "@nestjs/cli": "7.1.2",
    "@nestjs/schematics": "7.0.0",
    "@nestjs/testing": "7.0.8",
    "@types/express": "4.17.6",
    "@types/faker": "4.1.11",
    "@types/jest": "25.2.1",
    "@types/node": "13.11.1",
    "@types/passport-local": "1.0.33",
    "@types/supertest": "2.0.8",
    "@typescript-eslint/eslint-plugin": "2.28.0",
    "@typescript-eslint/parser": "2.28.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.2",
    "jest": "25.3.0",
    "prettier": "2.0.4",
    "supertest": "4.0.2",
    "ts-jest": "25.3.1",
    "ts-loader": "7.0.0",
    "ts-node": "8.8.2",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
